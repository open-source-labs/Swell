    "appId": "com.getswell.Swell.app",
    "extends": null,
    "files": [
      "dist/index.html",
      "dist/main.js",
      "dist/img/swell-bg__2JecU.png",
      "dist/img/Trashcan__3piXy.png",
      "dist/img/arrow_drop_down_white_192x192__3cp1B.png",
      "menu/mainMenu.js",
      "package.json",
      "main.js",
      "httpserver.js"
    ],
    "nodeVersion": "12.14.1",
    "nsis": {
      "createDesktopShortcut": "always"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "open-source-labs",
      "repo": "Swell",
      "private": false
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "moduleNameMapper": {
      "electron": "<rootDir>/__mocks__/electronMock.js",
